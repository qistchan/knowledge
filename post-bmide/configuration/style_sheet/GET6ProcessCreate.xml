<rendering>
    <page title="General" titleKey="tc_xrt_General">      
        <section title="Item Information" titleKey="tc_xrt_ItemInformation">
            <property name="item_id" />
            <property name="revision:item_revision_id" /> 
            <property name="object_name" />
            <property name="object_desc" />
            <separator/>
            <property name="uom_tag" /> 
              <property name="ip_classification" /> 
             <separator/>
            <!-- OOTB configuration item check box is not visibile, you can remove the comment once is_configuration_item in createInput is turned on in BMIDE -->
            <!--
            <property name="is_configuration_item" renderingHint="checkbox" />
            -->
           <property name="revision:get6Alternate" /> 
          <separator/>
          <property name="revision:get6Autocharge" />
             <separator/>
          <property name="revision:get6Checkpoint" />
              <separator/>
           <property name="revision:get6MaterialConsQuantity" />
               <separator/>
            <property name="revision:get6ScheduledFlag" />
        <separator/>
 </section>
        <section title="Additional Item Information" titleKey="tc_xrt_AdditionalItemInformation" initialstate="collapsed">
            <property name="IMAN_master_form:program_id" />
            <property name="IMAN_master_form:process_comment"  />
            <property name="IMAN_master_form:target_time" />
            <property name="IMAN_master_form:target_cost" />
        </section>

        <section title="Item Revision Information" titleKey="tc_xrt_ItemRevisionInformation" initialstate="collapsed">
            <property name="revision:IMAN_master_form_rev:program_id" />
            <property name="revision:IMAN_master_form_rev:process_comment" />
            <property name="revision:IMAN_master_form_rev:scheduled_time" />
            <property name="revision:IMAN_master_form_rev:scheduled_cost" />
        </section>
    </page>

</rendering>